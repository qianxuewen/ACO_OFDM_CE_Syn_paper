@Article{Jiang2015An,
  author   = {Jiang, Fangqing and Deng, Honggui and Xiao, Wei and Tao, Shaohua and Zhu, Kaicheng},
  title    = {An ICA based MIMO-OFDM VLC scheme},
  year     = {2015},
  volume   = {347},
  pages    = {37-43},
  abstract = {In this paper, we propose a novel ICA based MIMO-OFDM VLC scheme, where ICA is applied to convert the MIMO-OFDM channel into several SISO-OFDM channels to reduce computational complexity in channel estimation, without any spectral overhead. Besides, the FM is first investigated to further modulate the OFDM symbols to eliminate the correlation of the signals, so as to improve the separation performance of the ICA algorithm. In the 4脳4MIMO-OFDM VLC simulation experiment, LOS path and NLOS paths are both considered, each transmitting signal at 100 Mb/s. Simulation results show that the BER of the proposed scheme reaches the 10level at SNR=20 dB, which is a large improvement compared to the traditional schemes.},
