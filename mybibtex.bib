@Article{Du2016Channel,
  author   = {Du, Jie and Deng, Honggui and Qian, Xuewen and Zhang, Chaoyang},
  title    = {Channel estimation scheme based on compressed sensing and parameter estimation for an orthogonal frequency division multiplexing visible light communications system},
  year     = {2016},
  volume   = {55},
  number   = {11},
  pages    = {116109},
  abstract = {Abstract     Abstract  | Introduction  | System Model and Scheme Description  | Simulation Analysis  | Conclusion  | Acknowledgments  | References                   Abstract. 聽 In order to mitigate bandwidth attenuation of diffusion link visible light communication systems caused by multipath effects, we present an optical orthogonal frequency division multiplexing channel estimation scheme based on compressed sensing (CS) and estimation of signal parameters via rotational invariance techniques (ESPRIT). First, we derived a parametric channel model. Then, we used ESPRIT to obtain multipath channel parameters. After that, we built a dynamic over-complete dictionary that can be used in CS processing. Finally, we reconstructed the channel response by using a basis pursuit denoising algorithm to equalize the received signal in frequency domain. Compared with traditional schemes, the proposed scheme can improve channel estimation accuracy without increasing dictionary size. A set of computer simulations demonstrated the effectiveness of the proposed scheme.                       Figures in this Article        Conference Presentation Video        Visit SPIE.TV},
  journal  = {Optical Engineering},
  keywords = {Associative arrays;Compressed sensing;Matrices;Orthogonal frequency division multiplexing;Simulations;Visible radiation},
}

